!  font was "6x13"
Rxvt.background: Black
Rxvt.foreground: White
Rxvt.font: -xos4-termina-medium-r-normal--12-120-72-72-c-60-iso10646-1
Rxvt.scrollBar_right: True
Rxvt.saveLines: 5000

