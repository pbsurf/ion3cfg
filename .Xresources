Xft.dpi: 96.0
!Xft.antialias: 0
!Xft.hinting: 1
!Xft.hintstyle: hintfull
!Xft.rgba: none

!Xcursor.theme: flatcursor

UXTerm*scrollBar: True
UXTerm*rightScrollBar: True
UXTerm*saveLines: 5000

